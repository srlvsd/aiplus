def get_pascal_triangle(deep: int) -> list:
    row_0 = [1]
    row_1 = [1, 1]
    triangle = [row_0] + [row_1] + [[1] + [1]*i for i in range(2, deep + 1)]

    for i in range(2, len(triangle)):
        for j in range(1, len(triangle[i]) - 1):
            triangle[i][j] = triangle[i-1][j-1] + triangle[i-1][j]
    return triangle
